<form id="addVaccineForm" method="post" action="javascript:null;">
    <div class="hidden">
        <input type="number" name="id" title="ID">
    </div>
    <div class="form-group">
        <label class="control-label">Vaccine name:</label>
        <input type="text" class="form-control" name="title" placeholder="Vaccine name">
    </div>
    <div class="modal-footer">
        <!-- form output -->
        <div class="text-center text-info well-sm" id="error-box"></div>
        <!-- modal buttons -->
        <input type="submit" class="btn btn-primary btn-block" value="Add Vaccine"/>
    </div>
</form>

