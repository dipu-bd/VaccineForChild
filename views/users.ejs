<div class="container" id="users-page">
    <div class="navbar navbar-default">
        <div class="navbar-brand">List of all registered user</div>
        <div class="navbar-btn btn-group navbar-right right-pad">
            <button class="btn btn-default" id="refresh-button">
                <i class="glyphicon glyphicon-refresh"></i> Refresh
            </button>
            <button class="btn btn-default">

            </button>
        </div>
    </div>
    <div class="thumbnail">
        <table class="table table-bordered">
            <thead>
            <tr>
                <td>User Name</td>
                <td>Email</td>
                <td>Full Nme</td>
                <td>Address</td>
                <td>Phone</td>
                <td>Children registered</td>
                <td>Actions</td>
            </tr>
            </thead>
            <tbody id="user-list">
                <tr><td><div class="flex-full">Empty List</div></td></tr>
            </tbody>
        </table>
    </div>

    <table class="hidden">
        <tbody id="user-body">
        <tr id="user-wrapper">
            <td>
                <i class="fa fa-user" id="access"></i>
                <span id="uname"></span>
            </td>
            <td id="email"></td>
            <td id="name"></td>
            <td id="address"></td>
            <td id="phone"></td>
            <td id="childCount"></td>
            <td>
                <div class="btn-group">
                    <button class="btn btn-warning" id="sms-button">
                        <i class="fa fa-mobile-phone"></i> Send SMS
                    </button>
                    <button class="btn btn-warning" id="email-button">
                        <i class="fa fa-envelope"></i> Send Email
                    </button>
                </div>
                <div class="btn-group">
                    <button class="btn btn-warning" id="admin-button">
                        <i class="fa fa-lock"></i> Make Admin
                    </button>
                    <button class="btn btn-warning" id="delete-button">
                        <i class="fa fa-remove"></i> Delete User
                    </button>
                </div>
            </td>
        </tr>
        </tbody>
    </table>
</div>