<div class="modal-content polaroid">
    <div class="modal-header">
        <h3 class="modal-title">
            <i class="fa fa-child fa-2x"></i>
            <span id="name">Child Name</span>
        </h3>
    </div>
    <div class="modal-body">
        <dl class="dl-horizontal text-left">
            <dt>Age:</dt>
            <dd id="age">10 days old</dd>
            <dt>Birthday:</dt>
            <dd id="dab">Thu 17 Mar, 2016</dd>
            <dt>Height:</dt>
            <dd id="height">3.75"</dd>
            <dt>Weight:</dt>
            <dd id="weight">2.5kg</dd>
        </dl>
    </div>
    <div class="modal-footer">
        <div class="btn-group">
            <button id="edit" class="btn btn-primary">
                <i class="fa fa-edit"></i> Edit
            </button>
            <button id="delete" class="btn btn-warning">
                <i class="fa fa-remove"></i> Delete
            </button>
        </div>
    </div>
</div>